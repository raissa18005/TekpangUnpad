<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teknologipangan">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeknologiPangan">
        <activity android:name=".Fasilitas"></activity>
        <activity android:name=".VisiMisi" />
        <activity android:name=".Struktur" />
        <activity android:name=".Sejarah" />
        <activity android:name=".Dosen" />
        <activity android:name=".Lulusan" />
        <activity android:name=".Kurikulum" />
        <activity android:name=".More" />
        <activity android:name=".Deskripsi" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>